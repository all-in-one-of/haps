    multiparm {
	name	    vm_cryptolayers
	label	    "Cryptomatte Layers"
	default	    { 0 }
	parmtag	{ spare_category CRYPTO_OUTPUT_LABEL }
	parm {
	    name	"vm_cryptolayerenable#"
	    type	toggle
	    default	{ 1 }
	    nolabel
	    joinnext
	}
	parm {
            name	"vm_cryptolayerprop#"
            label	"Property"
            type	string
            default	{ "materialname" }
	    menureplace {
			"materialname"	 "Material Name"
			"name"		 "Object Name"
	    }
	}
	parm {
	    name	"vm_cryptolayername#"
            label	"Channel Name"
	    type	string
	    default	{ "CryptoMaterial" }
